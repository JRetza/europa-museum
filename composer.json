{
    "name": "craftcms/demo-europa-museum",
    "description": "Europa Museum Demo",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "project",
        "demo",
        "starter"
    ],
    "license": "0BSD",
    "homepage": "https://craftcms.com/demo",
    "type": "project",
    "support": {
        "email": "support@craftcms.com",
        "issues": "https://github.com/craftcms/demo-europa-museum/issues",
        "forum": "https://craftcms.stackexchange.com/",
        "source": "https://github.com/craftcms/demo-europa-museum",
        "docs": "https://github.com/craftcms/demo-europa-museum"
    },
    "require": {
        "craftcms/aws-s3": "^2.0.0-beta",
        "craftcms/cms": "4.0.x-dev",
        "craftcms/redactor": "^3.0.0-beta",
        "fakerphp/faker": "^1.14",
        "mead-steve/mono-snag": "^4.1",
        "nystudio107/craft-seomatic": "dev-develop-v4 as 4.0.0-beta.5",
        "nystudio107/craft-twigpack": "^4.0.0-beta.2",
        "nystudio107/craft-typogrify": "^4.0.0-beta.2",
        "pixelandtonic/yii2-dynamodb": "dev-develop",
        "sebastianlenz/linkfield": "dev-craft4 as 2.0",
        "solspace/craft-freeform": "dev-craft4 as 3.11.9",
        "verbb/default-dashboard": "dev-craft-4",
        "vlucas/phpdotenv": "^3.4.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "modules\\demos\\": "modules/demos/src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php craft db/restore seed.sql > /dev/null 2>&1",
            "@php craft demos/seed > /dev/null 2>&1"
        ]
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/timkelty/craft-imager-x"
        },
        {
            "type": "git",
            "url": "https://github.com/timkelty/craft-linkfield"
        },
        {
            "type": "git",
            "url": "https://github.com/timkelty/craft-utils"
        },
        {
            "type": "git",
            "url": "https://github.com/brandonkelly/craft3-freeform"
        }
    ],
    "prefer-stable": true
}
